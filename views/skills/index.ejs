<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%=title%></title>
  <link rel="stylesheet" href="/stylesheets/style.css">
</head>
<body>
  <h1> <%=title%> as of <%=time%></h1>
  <% if(error) { %>
    <h2><%= error %></h2>
  <% } %>
  <ul>
    <% devolperSkills.forEach(function(skill) { %>
      <li>
        <form 
        action="/devSkills/<%= skill._id %>?_method=DELETE"
        class="delete-form"
        method="POST"
      >
        <button type="submit">X</button>
      </form>
        <a href="/devSkills/<%=skill._id %>"><%= skill.text %></a>
          - 
        <%= skill.done ? 'Expert' : 'noob' %>
      </li>
    <% }) %>
  </ul>
  <a href="/devSkills/new">Add a newly aquired skill</a>
</body>
</html>